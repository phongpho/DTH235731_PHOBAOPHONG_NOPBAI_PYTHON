    "print('Chương trình kiểm tra năm nhuần')\n",
    "year = int(input('Mời nhập năm cần kiểm tra: '))\n",
    "if(year % 4 == 0 and year % 100 != 0) or (year % 400 == 0):\n",
    "    print(f'Năm {year} là năm nhuần')\n",
    "else:\n",
    "    print(f'Năm {year} không phải là năm nhuần')\n"
